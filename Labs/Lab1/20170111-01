[33m 46 
 47 [m[34m/* This file defines `struct dirent'.[m
[33m 48 
 49 [m[34m   It defines the macro `_DIRENT_HAVE_D_NAMLEN' iff there is a `d_namlen'[m
[33m 50 [m[34m   member that gives the length of `d_name'.[m
[33m 51 
 52 [m[34m   It defines the macro `_DIRENT_HAVE_D_RECLEN' iff there is a `d_reclen'[m
[33m 53 [m[34m   member that gives the size of the entire directory entry.[m
[33m 54 
 55 [m[34m   It defines the macro `_DIRENT_HAVE_D_OFF' iff there is a `d_off'[m
[33m 56 [m[34m   member that gives the file offset of the next directory entry.[m
[33m 57 
 58 [m[34m   It defines the macro `_DIRENT_HAVE_D_TYPE' iff there is a `d_type'[m
[33m 59 [m[34m   member that gives the type of the file.[m
[33m 60 [m[34m */[m[61;87H1,1[11CTop[1;5H[?12l[?25hP+q436f\P+q6b75\P+q6b64\P+q6b72\P+q6b6c\P+q2332\P+q2334\P+q2569\P+q2a37\P+q6b31\[?25l[27m[m[H[2J[1;1H[33m 59 [m[34m   member that gives the type of the file.[m
[33m 60 [m[34m */[m
[33m 61 
 62 [m[35m#include [m[31m<bits/dirent.h>[m
[33m 63 
 64 [m[35m#if (defined __USE_BSD || defined __USE_MISC) && !defined d_fileno[m
[33m 65 [m[35m# define d_ino  d_fileno         [m[34m/* Backward compatibility.  */[m
[33m 66 [m[35m#endif[m
[33m 67 
 68 [m[34m/* These macros extract size information from a `struct dirent *'.[m
[33m 69 [m[34m   They may evaluate their argument multiple times, so it must not[m
[33m 70 [m[34m   have side effects.  Each of these may involve a relatively costly[m
[33m 71 [m[34m   call to `strlen' on some systems, so these values should be cached.[m
[33m 72 
 73 [m[34m   _D_EXACT_NAMLEN (DP) returns the length of DP->d_name, not including[m
[33m 74 [m[34m   its terminating null character.[m
[33m 75 
 76 [m[34m   _D_ALLOC_NAMLEN (DP) returns a size at least (_D_EXACT_NAMLEN (DP) + 1);[m
[33m 77 [m[34m   that is, the allocation size needed to hold the DP->d_name string.[m
[33m 78 [m[34m   Use this macro when you don't need the exact length, just an upper bound.[m
[33m 79 [m[34m   This macro is less likely to require calling `strlen' than _D_EXACT_NAMLEN.[m
[33m 80 [m[34m   */[m
[33m 81 
 82 [m[35m#ifdef _DIRENT_HAVE_D_NAMLEN[m
[33m 83 [m[35m# define _D_EXACT_NAMLEN(d) ((d)->d_namlen)[m
[33m 84 [m[35m# define _D_ALLOC_NAMLEN(d) (_D_EXACT_NAMLEN (d) + [m[31m1[m[35m)[m
[33m 85 [m[35m#else[m
[33m 86 [m[35m# define _D_EXACT_NAMLEN(d) (strlen ((d)->d_name))[m
[33m 87 [m[35m# ifdef _DIRENT_HAVE_D_RECLEN[m
[33m 88 [m[35m#  define _D_ALLOC_NAMLEN(d) ((([m[32mchar[m[35m *) (d) + (d)->d_reclen) - &(d)->d_name[[m[31m0[m[35m])[m
[33m 89 [m[35m# else[m
[33m 90 [m[35m#  define _D_ALLOC_NAMLEN(d) ([m[33msizeof[m[35m (d)->d_name > [m[31m1[m[35m ? [m[33msizeof[m[35m (d)->d_name : \[m
[33m 91 [m[35m                  _D_EXACT_NAMLEN (d) + [m[31m1[m[35m)[m
[33m 92 [m[35m# endif[m
[33m 93 [m[35m#endif[m
[33m 94 
 95 
 96 [m[35m#ifdef __USE_BSD[m
[33m 97 [m[34m/* File types for `d_type'.  */[m
[33m 98 [m[32menum[m
[33m 99 [m  {
[33m100 [m    DT_UNKNOWN = [31m0[m,
