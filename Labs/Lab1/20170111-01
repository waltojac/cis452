 309 [m[34m/* Values for the WHENCE argument to lseek.  */[m
[33m 310 [m[35m#ifndef _STDIO_H        [m[34m/* <stdio.h> has the same definitions.  */[m
[33m 311 [m[35m# define [m[30m[43mSEEK_SET[m[35m   [m[31m0[m[35m   [m[34m/* Seek from beginning of file.  */[m
[33m 312 [m[35m# define [m[31mSEEK_CUR[m[35m   [m[31m1[m[35m   [m[34m/* Seek from current position.  */[m
[33m 313 [m[35m# define [m[31mSEEK_END[m[35m   [m[31m2[m[35m   [m[34m/* Seek from end of file.  */[m
[33m 314 [m[35m# ifdef __USE_GNU[m
[33m 315 [m[35m#  define SEEK_DATA [m[31m3[m[35m   [m[34m/* Seek to next data.  */[m
[33m 316 [m[35m#  define SEEK_HOLE [m[31m4[m[35m   [m[34m/* Seek to next hole.  */[m
[33m 317 [m[35m# endif[m
[33m 318 [m[35m#endif[m
[33m 319 
 320 [m[35m#if defined __USE_BSD && !defined L_SET[m
[33m 321 [m[34m/* Old BSD names for the same constants; just for compatibility.  */[m
[33m 322 [m[35m# define L_SET      [m[30m[43mSEEK_SET[m
[33m 323 [m[35m# define L_INCR     [m[31mSEEK_CUR[m
[33m 324 [m[35m# define L_XTND     [m[31mSEEK_END[m
[33m 325 [m[35m#endif[m
[33m 326 
 327 
 328 [m[34m/* Move FD's file position to OFFSET bytes from the[m
[33m 329 [m[34m   beginning of the file (if WHENCE is [m[30m[43mSEEK_SET[m[34m),[m
[33m 330 [m[34m   the current position (if WHENCE is SEEK_CUR),[m
[33m 331 [m[34m   or the end of the file (if WHENCE is SEEK_END).[m
[33m 332 [m[34m   Return the new file position.  */[m
[33m 333 [m[35m#ifndef __USE_FILE_OFFSET64[m
[33m 334 [m[32mextern[m __off_t lseek ([32mint[m __fd, __off_t __offset, [32mint[m __whence) __THROW;
