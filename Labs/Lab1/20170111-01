[33m 31 
 32 [m[35m#ifdef __USE_XOPEN[m
[33m 33 [m[35m# ifndef __ino_t_defined[m
[33m 34 [m[35m#  ifndef __USE_FILE_OFFSET64[m
[33m 35 [m[32mtypedef[m __ino_t ino_t;
