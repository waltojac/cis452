[?1049h[?1h=[2;1H‚ñΩ[6n[1;1H  [1;1H[1;61r[?12;25h[?12l[?25h[27m[m[H[2J[?25l[61;1H"sample2.c" 26L, 434C[>c[1;1H[33m  1 [m[35m#include [m[31m<mcheck.h>[m
[33m  2 [m[35m#include [m[31m<stdio.h>[m
[33m  3 [m[35m#include [m[31m<stdlib.h>[m
